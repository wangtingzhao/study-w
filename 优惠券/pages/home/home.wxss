.home_content{
    padding-top: 80px;
}
.home_fixed{
    position: fixed;
    width: 100%;
    top: 0;
    left: 0;
    z-index: 1;
    background: #fff;
}
.url_img{
    width:100%;
}
.gosearch{
    width: 100%;
    height: 40px;
    background: #ff98b1;
    position: relative;
}
.safter::after{
    content: '输入关键词或粘贴淘宝/天猫商品标题';
    display: block;
    position: absolute;
    width: 90%;
    height: 30px;
    line-height: 30px;
    font-size: 12px;
    color:#fff;
    background: rgba(255,255,255,0.5);
    border-radius: 2.5px;
    margin:auto;
    top: 0;left: 0;
    bottom: 0; right: 0;
    text-indent: 11%;
}
.search_img{
    width: 20px;
    height: 20px;
    position: absolute;
    left: 8%;
    top:50%;
    transform: translateY(-50%);
}
.swper_flex{
    display: flex;
    height: 40px;
    line-height: 40px;
}
.flex{
    width:80px!important;
    flex: 1!important;
}
.category_item{
    width: 60px;
    height: 100%;
    flex: 1;
    display: inline-block;
    text-align: center;

}
.cate_btn{
    width: 100%;
    overflow: hidden;
    padding: 10px 0;
}
.cate_child{
    width: 20%;
    height:50px;
    float: left;
    padding: 3px 0;
}
.cate_img{
    width: 30px;
    height: 30px;
    display: block;
    margin: 0 auto;
}
.cate_title{
    font-size: 12px;
    color:#606060;
    text-align: center;
}
.seckill_title{
    font-size: 12px;
    color:#e9302d;
    display: flex;
    height: 20px;
    line-height: 20px;
    padding: 0 10px;
}
.seckill_time{
    padding: 0 5px;
    color:#fff;
    background: #222;
    border-radius: 2.5px;
}
.seckill_img,.Simg_right{
    width: 15px;
    height: 15px;
    vertical-align: middle;
}
.seckill_left{
    width: 75px;
}
.seckill_right{
    flex: 1;
    text-align: right;
}
.Simg_right{
    margin-left: 10px;
}
.goods_img{
    width: 130px;
    height: 130px;
}
.goods_name{
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    color: #646464;
}
.goods_price{
    font-size:16px;
    color: #e9302d;
}
.price_title{
    display: inline-block;
    color:#cacaca;
    font-size: 12px;
    margin-left: 10px;
}
.goods_list{
    padding:0 10px;
}
